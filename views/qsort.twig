	{% set names = question_template_attribute.area_names|split(',')  %}
    {% set limits = question_template_attribute.area_limits|split(',')  %}
		
	<div class="q-sort-outer-wrapper">
        <div class="droppable items-start"></div>
        <div class="items-end-wrapper">
        {% for index, item in names %}
            <div class="items-end-inner">
                <div class="items-end-text">{{item|trim}}</div>
                <div class="droppable items-end items-end-{{index+1}}" data-items-end="{{index+1}}" data-items-limit="{{limits[index]|trim}}"></div>
            </div>
        {% endfor %}			
        </div>
	</div>
